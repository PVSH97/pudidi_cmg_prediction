{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index_fixed.html"
    },
    {
      "source": "/api",
      "destination": "/api/index"
    },
    {
      "source": "/api/predictions/live",
      "destination": "/api/predictions_live"
    },
    {
      "source": "/api/cmg/current",
      "destination": "/api/cmg/current"
    },
    {
      "source": "/api/cache/(.*)",
      "destination": "/api/cache"
    }
  ],
  "functions": {
    "api/cmg/*.py": {
      "maxDuration": 30
    },
    "api/*.py": {
      "includeFiles": "data/cache/**",
      "maxDuration": 30
    }
  },
  "cleanUrls": true,
  "trailingSlash": false
}