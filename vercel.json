{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index_fixed.html"
    },
    {
      "source": "/api",
      "destination": "/api/index"
    },
    {
      "source": "/api/test",
      "destination": "/api/test"
    },
    {
      "source": "/api/predictions/live",
      "destination": "/api/predictions_live"
    },
    {
      "source": "/api/cmg/current",
      "destination": "/api/cmg/current"
    },
    {
      "source": "/api/cmg/refresh",
      "destination": "/api/cmg/refresh"
    },
    {
      "source": "/api/cmg/status",
      "destination": "/api/cmg/status"
    },
    {
      "source": "/api/cache/(.*)",
      "destination": "/api/cache"
    }
  ],
  "functions": {
    "api/cmg/*.py": {
      "maxDuration": 30
    },
    "api/*.py": {
      "includeFiles": "data/cache/**"
    }
  }
}